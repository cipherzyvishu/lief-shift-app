{
  "name": "lief-shift-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:reset": "prisma migrate reset --force",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "check-locations": "node scripts/check-locations.js",
    "validate-deployment": "node scripts/validate-deployment.js",
    "prebuild": "prisma generate && node scripts/check-locations.js",
    "type-check": "tsc --noEmit",
    "build:check": "npm run type-check && npm run lint && npm run build",
    "deploy-prep": "node -e \"console.log('Run scripts/deploy-prep.bat (Windows) or scripts/deploy-prep.sh (Linux/Mac)')\"",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@apollo/client": "^3.13.9",
    "@apollo/server": "^5.0.0",
    "@as-integrations/next": "^4.0.0",
    "@auth0/nextjs-auth0": "^4.9.0",
    "@prisma/client": "^6.13.0",
    "antd": "^5.26.7",
    "chart.js": "^4.5.0",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "prisma": "^6.13.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
